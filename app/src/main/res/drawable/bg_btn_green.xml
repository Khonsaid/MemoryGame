<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="2dp"
        android:end="2dp"
        android:start="2dp"
        android:top="2dp">
        <shape android:shape="rectangle">
            <solid android:color="#29B715" />
            <corners android:radius="10dp" />
        </shape>
    </item>
    <item
        android:bottom="5dp"
        android:end="3dp"
        android:start="3dp"
        android:top="3dp">
        <shape android:shape="rectangle">
            <padding
                android:bottom="5dp"
                android:left="10dp"
                android:right="10dp"
                android:top="5dp" />
            <solid android:color="@color/light_green" />
            <corners android:radius="10dp" />
        </shape>
    </item>
</layer-list>